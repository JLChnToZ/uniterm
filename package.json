    "isexe": "^2.0.0",
    "js-yaml": "^3.13.1",
    "lazy-initializer": "^0.2.0",
    "msgpack-lite": "^0.1.26",
    "node-pty": "^0.8.1",
    "rxjs": "^6.4.0",
