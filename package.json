{
  "name": "uniterm",
  "version": "0.13.0",
  "description": "Universal Terminal Emulator",
  "main": "./lib/main.js",
  "scripts": {
    "build": "electron-rebuild && tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron .",
    "rebuild": "electron-rebuild -f",
    "package": "tsc && node .packager.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JLChnToZ/uniterm.git"
  },
  "keywords": [
    "terminal",
    "tty",
    "hterm"
  ],
  "author": "Jeremy Lam \"JLChnToZ\"",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JLChnToZ/uniterm/issues"
  },
  "homepage": "https://github.com/JLChnToZ/uniterm#readme",
  "devDependencies": {
    "@types/hyperscript": "0.0.3",
    "@types/js-yaml": "^3.11.2",
    "@types/shell-escape": "^0.2.0",
    "@types/yargs": "^12.0.1",
    "electron": "^3.0.9",
    "electron-packager": "^12.2.0",
    "electron-prebuilt-compile": "3.0.6",
    "electron-rebuild": "^1.8.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@types/gist-package-json": "git+https://gist.github.com/5c1cc527fe6b5b7dba41fec7fe54bf6e.git",
    "@types/which": "^1.3.1",
    "about-window": "^1.12.1",
    "code-to-signal": "^1.0.2",
    "default-shell": "^1.0.1",
    "hyperscript": "^2.0.2",
    "isexe": "^2.0.0",
    "js-yaml": "^3.12.0",
    "node-pty": "^0.8.0",
    "shell-escape": "^0.2.0",
    "tocas-ui": "^2.3.2",
    "which": "^1.3.1",
    "wslpty": "^0.2.1",
    "xterm": "^3.8.0",
    "yargs": "^12.0.2"
  }
}
