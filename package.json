  "bugs": {
    "url": "https://github.com/JLChnToZ/uniterm/issues"
  },
  "homepage": "https://github.com/JLChnToZ/uniterm#readme",
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-packager": "^13.1.1",
    "electron-prebuilt-compile": "^4.0.0",
    "electron-rebuild": "^1.8.4",
    "tslint": "^5.15.0",
    "typescript": "^3.4.2"
  },
  "dependencies": {
    "@types/chokidar": "^2.1.3",
